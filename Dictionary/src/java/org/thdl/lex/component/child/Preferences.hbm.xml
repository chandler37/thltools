<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>



<class name="org.thdl.lex.NewPreferences" table="Preferences">
	<id name="id" type="java.lang.Integer" column="id">
		<generator class="native" />
	</id>
	<property name="userId" type="java.lang.Integer" column="userId" not-null="true" length="11"/>
	<property name="preferencesName" type="java.lang.Integer" column="preferencesName" length="11"/>
	<property name="projectSubject" type="java.lang.Integer" column="projectSubject" length="11"/>
	<property name="source" type="java.lang.Integer" column="source" length="11"/>
	<property name="language" type="java.lang.Integer" column="language" length="11"/>
	<property name="script" type="java.lang.Integer" column="script" length="11"/>
	<property name="dialect" type="java.lang.Integer" column="dialect" length="11"/>
	<property name="note" type="java.lang.String" column="note" length="65535"/>
	<property name="useDefaultProjSub" type="java.lang.Boolean" column="useDefaultProjSub" not-null="true" length="5"/>
	<property name="useDefaultSource" type="java.lang.Boolean" column="useDefaultSource" not-null="true" length="5"/>
	<property name="useDefaultLanguage" type="java.lang.Boolean" column="useDefaultLanguage" not-null="true" length="5"/>
	<property name="useDefaultScript" type="java.lang.Boolean" column="useDefaultScript" not-null="true" length="5"/>
	<property name="useDefaultDialect" type="java.lang.Boolean" column="useDefaultDialect" not-null="true" length="5"/>
	<property name="useDefaultNote" type="java.lang.Boolean" column="useDefaultNote" not-null="true" length="5"/>
	<property name="projectSubjectSet" type="java.lang.String" column="projectSubjectSet" length="255"/>
	<property name="sourceSet" type="java.lang.String" column="sourceSet" length="255"/>
	<property name="languageSet" type="java.lang.String" column="languageSet" length="255"/>
	<property name="scriptSet" type="java.lang.String" column="scriptSet" length="255"/>
	<property name="dialectSet" type="java.lang.String" column="dialectSet" length="255"/>

	<!-- associations -->

</class>
</hibernate-mapping>
