<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<class name="org.thdl.lex.component.child.ProjectSubject" table="ProjectSubjects">
	<id name="id" type="java.lang.Integer" column="id">
		<generator class="native" />
	</id>
	<property name="projectSubject" type="java.lang.String" column="projectSubject" length="255"/>
	<property name="leader" type="java.lang.Integer" column="leader" not-null="true" length="11"/>
	<property name="participantList" type="java.lang.String" column="participantList" length="65535"/>

	<!-- associations -->

</class>
</hibernate-mapping>
